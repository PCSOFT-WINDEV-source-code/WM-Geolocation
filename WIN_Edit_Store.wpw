      -
        code : |1-
         HReset(Store)
         
         if EDT_Name = "" then
         	Error("Specify a store name.")
         	return
         end
         
         ScreenToFile(WIN_Edit_Store,Store)
         
         //Retrieve the GPS position 
         arrAddresses is array of Address
         if geoGetAddress(EDT_Latitude + "," + EDT_Longitude, arrAddresses) = False then
         	if ErrorOccurred then 
         		Error(ErrorInfo()) 
         		return
         	end
         end
         StoreAddress is an Address = arrAddresses[1]
         Store.City = Upper(StoreAddress..City)
         Store.Address = StoreAddress..Street
         
         // New store
         if not HAdd(Store) then
         	Error(HErrorInfo())
         	return
         end
         
         MyWindow..ReturnedValue = Store.StoreID
         Close()
        type : 18
